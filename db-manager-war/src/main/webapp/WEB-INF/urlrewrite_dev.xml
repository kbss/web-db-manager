<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule>
        <from>
            ^\/((?!\/|index_dev.html).)*$
        </from>
        <to last="true">index_dev.html</to>
    </rule>
    <rule>
        <from>^/$</from>
        <to last="true">index_dev.html</to>
    </rule>
</urlrewrite>
